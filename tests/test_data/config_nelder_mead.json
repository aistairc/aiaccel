{
  "generic": {
    "workspace": "/tmp/work",
    "job_command": "python original_main.py"
  },
  "resource": {
    "type": "local",
    "num_node": 4
  },
  "ABCI": {
    "job_script_preamble": "wrapper_abci.sh",
    "group": "gaa"
  },
  "optimize": {
    "search_algorithm": "aiaccel.optimizer.NelderMeadOptimizer",
    "goal": "minimize",
    "trial_number": 10,
    "parameters": [
      {
        "name": "x1",
        "type": "uniform_float",
        "log": false,
        "lower": -1.0,
        "upper": 1.0,
        "initial": [
          0.81,
          0.72,
          -0.41
        ]
      },
      {
        "name": "x2",
        "type": "uniform_float",
        "log": false,
        "lower": -1.0,
        "upper": 1.0,
        "initial": [
          0.61,
          0.62,
          -0.56
        ]
      }
    ]
  },
  "job_setting": {
    "name_length": 6,
    "cancel_retry": 3,
    "cancel_timeout": 60,
    "expire_retry": 3,
    "expire_timeout": 60,
    "finished_retry": 3,
    "finished_timeout": 60,
    "job_retry": 2,
    "job_timeout": 60,
    "kill_retry": 3,
    "kill_timeout": 60,
    "result_retry": 1,
    "runner_retry": 3,
    "runner_timeout": 60,
    "running_retry": 3,
    "running_timeout": 60
  },
  "logger": {
    "file": {
      "master": "master.log",
      "scheduler": "scheduler.log",
      "optimizer": "optimizer.log"
    },
    "log_level": {
      "master": "DEBUG",
      "scheduler": "DEBUG",
      "optimizer": "DEBUG"
    },
    "stream_level": {
      "master": "DEBUG",
      "scheduler": "DEBUG",
      "optimizer": "DEBUG"
    }
  },
