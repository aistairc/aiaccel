        {
          "name": "x5",
          "type": "uniform_float",
          "log": false,
          "lower": -5.0,
          "upper": 5.0,
          "default": 4.0
        },
        {
          "name": "x6",
          "type": "uniform_float",
          "log": false,
          "lower": -5.0,
          "upper": 5.0,
          "default": 4.0
        },
        {
          "name": "x7",
          "type": "uniform_float",
          "log": false,
          "lower": -5.0,
          "upper": 5.0,
          "default": 4.0
        },
        {
          "name": "x8",
          "type": "uniform_float",
          "log": false,
          "lower": -5.0,
